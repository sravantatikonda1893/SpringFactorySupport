<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
"http://www.springfranework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<bean id="t" class="beans.Test" factory-method="getInstance"/>
	
	<!-- As the Calendar is an abstract method, it cannot be instantiated directly
	For this, it has provided with getInstance() factory method in jdk api.
	-->
	
	<bean id="c" class="java.util.Calender" factory-method="getInstance"/>
</beans>